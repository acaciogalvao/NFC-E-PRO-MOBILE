{
  "name": "nfc-e-pro",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "server": "nodemon --exec ts-node server/src/index.ts",
    "build": "vite build && tsc -p server/tsconfig.json",
    "preview": "vite preview",
    "test": "vitest run",
    "dev:all": "concurrently \"pnpm run dev\" \"pnpm run server\"",
    "clean": "rm -rf dist server/dist",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css}\" \"server/src/**/*.ts\""
  },
  "dependencies": {
    "@google/genai": "^1.35.0",
    "@google/generative-ai": "^0.24.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "html2canvas": "1.4.1",
    "jspdf": "2.5.1",
    "lucide-react": "^0.561.0",
    "mongoose": "^9.1.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/mongoose": "^5.11.96",
    "@types/node": "^22.19.7",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/ui": "^4.0.17",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "tailwindcss": "^4.1.18",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-pwa": "^1.2.0",
    "vitest": "^4.0.17"
  }
}
